# SimDock Pro Environment Configuration
# Copy this file to .env and modify as needed

# ============================================
# API Configuration
# ============================================

# Optional CORS proxy for external API calls
# VITE_CORS_PROXY=https://your-cors-proxy.com

# ============================================
# Development Settings
# ============================================

# Enable debug logging
VITE_DEBUG=false

# Development server port (default: 5173)
# VITE_PORT=5173

# ============================================
# Feature Flags
# ============================================

# Enable experimental features
VITE_EXPERIMENTAL=false
